<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="er_crows_foot.ecore#//ERCFDiagram/entities"/>
    <ownedChild>
      <domainMetaElement
          href="er_crows_foot.ecore#//ERCFEntity"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="er_crows_foot.gmfgraph#ERCFEntityName"/>
        <features
            href="er_crows_foot.ecore#//ERCFEntity/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="er_crows_foot.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="er_crows_foot.gmfgraph#ERCFEntity"/>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0">
        <containmentFeature
            href="er_crows_foot.ecore#//ERCFEntity/attributes"/>
        <ownedChild>
          <domainMetaElement
              href="er_crows_foot.ecore#//ERCFAttribute"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="er_crows_foot.gmfgraph#ERCFAttributeName"/>
            <features
                href="er_crows_foot.ecore#//ERCFAttribute/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="er_crows_foot.gmftool#//@palette/@tools.0/@tools.1"/>
          <diagramNode
              href="er_crows_foot.gmfgraph#ERCFAttribute"/>
        </ownedChild>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.0">
        <compartment
            href="er_crows_foot.gmfgraph#ERCFEntityAttributesCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="er_crows_foot.ecore#//ERCFRelationship"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping">
      <diagramLabel
          href="er_crows_foot.gmfgraph#ERCFRelationshipNameExternalLabel"/>
      <features
          href="er_crows_foot.ecore#//ERCFRelationship/name"/>
    </labelMappings>
    <containmentFeature
        href="er_crows_foot.ecore#//ERCFDiagram/relationships"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="er_crows_foot.gmftool#//@palette/@tools.0/@tools.2"/>
    <diagramLink
        href="er_crows_foot.gmfgraph#ERCFRelationship"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="er_crows_foot.ecore#//ERCFRelationship/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="er_crows_foot.ecore#//ERCFRelationship/target"/>
  </links>
  <diagram>
    <diagramCanvas
        href="er_crows_foot.gmfgraph#er_crows_foot"/>
    <domainModel
        href="er_crows_foot.ecore#/"/>
    <domainMetaElement
        href="er_crows_foot.ecore#//ERCFDiagram"/>
    <palette
        href="er_crows_foot.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
